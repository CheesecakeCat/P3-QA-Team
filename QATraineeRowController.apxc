public class QATraineeRowController {
	@AuraEnabled
    public static Training_Assignment__c SingleTrainee(Id Trainee_Id) //This becomes trainingAssignmentID
    {
        return [SELECT Id, Name, Contact__r.FirstName, Contact__r.LastName FROM Training_Assignment__c 
                WHERE Id = :Trainee_Id LIMIT 1];
    }
}